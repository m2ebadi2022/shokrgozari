		pan_main2.SetLayoutAnimated(100,100%x,0,100%x,pan_main3.Height)
	Else If (current_indexPage=0) Then
		pan_main3.SetLayoutAnimated(100,-(100%x),0,100%x,pan_main3.Height)
	End If
	
	
	current_indexPage=1
	



	For i=1 To 28
		
		If(myFunc.get_State_tamrin1_byRoz(i).As(Int)<10)Then
			this_day=i
			lbl_show_tamrin_roz.Text=i&"/28"
			Return
		Else
			this_day=29
			lbl_show_tamrin_roz.TextSize=22
			lbl_show_tamrin_roz.Text="دفتر شکرگزاری"
		End If
		
	Next
	
	
	
