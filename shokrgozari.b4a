		If(myFunc.get_State_tamrin1_byRoz(i).As(Int)<10)Then
			this_day=i
			lbl_show_tamrin_roz.Text=i&"/28"
			Return
		Else
			this_day=29
			lbl_show_tamrin_roz.TextSize=22
			lbl_show_tamrin_roz.Text="دفتر شکرگزاری"
		End If
		
	Next
	
	
	
