Sub fill_list_tamrinat
	Dim counter_id As Int=0
	For i=1 To 6
		
		p = xui.CreatePanel("p")
		
		p.SetLayoutAnimated(0, 0, 0, 100%x, 80dip)
		p.LoadLayout("item_clv_tamrinat")
		
		
		clv_tamrinat_tagvim.Add(p,i)
		
			counter_id=counter_id+1
			L1.Text=counter_id
			r1.Tag=counter_id
			r1.Color=chek_state(counter_id)
			
			counter_id=counter_id+1
			L2.Text=counter_id
			r2.Tag=counter_id
			r2.Color=chek_state(counter_id)
		
			counter_id=counter_id+1
			L3.Text=counter_id
			r3.Tag=counter_id
			r3.Color=chek_state(counter_id)
		
			counter_id=counter_id+1
			L4.Text=counter_id
			r4.Tag=counter_id
			r4.Color=chek_state(counter_id)
		
			counter_id=counter_id+1
			L5.Text=counter_id
			r5.Tag=counter_id
			r5.Color=chek_state(counter_id)
		
		If(i=6)Then
			r4.Visible=False
			r5.Visible=False
		End If
	
	
	
	
	
	
		
	Next
	
	
	
End Sub


Sub chek_state (id_roz As Int) As String
	Dim color_str As String=""
	Dim code_state As Int = myFunc.get_State_tamrin1_byRoz(id_roz)
	
	If(code_state=10)Then
		color_str=0xFF66DF64
	Else If(code_state = 0 )Then
		color_str=0xFF919191
	Else
		color_str=0xFFDFCE64
	End If
	
	Return color_str
End Sub


Sub get_tamrin1_for_lsv(id As Int)
	lsv_roz.Clear
	lsv_roz.TwoLinesLayout.Label.Width=90%x
	lsv_roz.TwoLinesLayout.Label.TextColor=Colors.Green
	
	lsv_roz.TwoLinesLayout.SecondLabel.Width=90%x
	
	lbl_roz_lsv.Text="روز "&id
	Dim strFunc As StringFunctions
	strFunc.Initialize


	Dim ls_tamrin1_Roz As List
	ls_tamrin1_Roz.Initialize
	ls_tamrin1_Roz=myFunc.get_tamrin1_byRoz(id)
	
	For i=0 To ls_tamrin1_Roz.Size-1
		
		Dim list_num As List
		list_num.Initialize
		list_num=strFunc.Split(ls_tamrin1_Roz.Get(i) , "@")
		
		If(list_num.Get(2)<>"null"  And list_num.Get(2)<>"" )Then
		
			lsv_roz.AddTwoLines((i+1)&" : "&list_num.Get(2),list_num.Get(3))
		End If
		
	Next
	
	If(lsv_roz.Size=0)Then
	
		lbl_back_lsv_Click
		
		Msgbox2Async("اطلاعاتی ثبت نشده است!", "", "باشه", "", "", Null, False)
		Wait For Msgbox_Result (result As Int)
		If result = DialogResponse.POSITIVE Then
			
		End If
		
		
		
	End If
	
	
End Sub

